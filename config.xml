<?xml version='1.0' encoding='utf-8'?>
<widget id="com.notifi.iglecentro" version="2.0" versionCode="18" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Iglecentro</name>
    <description>Aplicaci√≥n movil de la iglesia cristiana Iglecentro .</description>
    <author email="getsemaidevelopers" href="#">Getsemani Developers Team</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="resources/android/icon/36.png" />
        <icon density="mdpi" src="resources/android/icon/48.png" />
        <icon density="hdpi" src="resources/android/icon/72.png" />
        <icon density="xhdpi" src="resources/android/icon/96.png" />
        <icon density="xxhdpi" src="resources/android/icon/144.png" />
        <icon density="xxxhdpi" src="resources/android/icon/192.png" />
        <splash density="port-ldpi" src="resources/android/splash/320.png" />
        <splash density="port-mdpi" src="resources/android/splash/480.png" />
        <splash density="port-hdpi" src="resources/android/splash/800.png" />
        <splash density="port-xhdpi" src="resources/android/splash/1280.png" />
        <splash density="port-xxhdpi" src="resources/android/splash/1600.png" />
        <splash density="port-xxxhdpi" src="resources/android/splash/1900.png" />
    </platform>
    <plugin name="cordova-plugin-device" spec="^2.0.2" />
    <plugin name="cordova-plugin-inappbrowser" spec="^3.0.0" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
    <plugin name="cordova-plugin-ionic-webview" spec="2.0.0-beta.1" />
    <plugin name="cordova-plugin-statusbar" spec="^2.4.2" />
    <plugin name="cordova-plugin-ionic-keyboard" spec="^2.1.2" />
    <engine name="ios" spec="^4.5.5" />
    <engine name="android" spec="7.0.0" />
</widget>
